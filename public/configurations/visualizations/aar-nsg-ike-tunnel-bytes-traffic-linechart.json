{
    "id": "aar-nsg-ike-tunnel-packet-bytes-linechart",
    "graph": "MultiLineGraph",
    "title": "Total Bytes",
    "description": "Line graph displays the throughput of the tunnel over the selected path",
    "author": "Bharat Mukheja",
    "creationDate": "3/6/2018",
    "data": {
        "dateHistogram": true,
        "xColumn": "date_histo",
        "yColumn": "{{ikeBytesTypeColumn}}",
        "yTickFormat": ".2s",
        "xLabel": "Time",
        "yLabel": "{{ikeBytesType}} Packets",
        "linesColumn": "tunnel",
        "showNull": false,
        "stroke": {
          "color": "#f76159",
          "width": "2px"
        },
        "legend": {
            "orientation": "horizontal",
            "show": true,
            "circleSize": 5,
            "labelOffset": 5
        },
        "tooltip": [
            { "column": "yColumn","label":"Total Bytes" },
	    { "column": "linesColumn","label":"Tunnel"}
        ]
    },
    "query": "aar-nsg-ike-tunnel-bytes-traffic-linechart",
    "filterOptions": {
        "Packet Type": {
            "parameter": "ikeBytesType",
            "default": "Tx",
            "options": [
                {
                    "label": "Tx",
                    "value": "Tx",
                    "default": true,
                    "forceOptions": {
                        "ikeBytesTypeColumn": "TxBytesCount"
                    }
                },
                {
                    "label": "Rx",
                    "value": "Rx",
                    "forceOptions": {
                        "ikeBytesTypeColumn": "RxBytesCount"
                    }
                }
            ]
        }
    }
}
