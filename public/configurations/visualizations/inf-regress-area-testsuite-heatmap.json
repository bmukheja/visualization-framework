{
    "id": "inf-regress-area-testsuite-heatmap",
    "graph": "HeatmapGraph",
    "title": "Last 15 days preferred regression combinations run history",
    "description": "The heatmap graph 15 day history of regressions preferred by AQL team. These regressions mean a combination of testsuite and arguments.\n\n Empty: The regression was not run.\nPASS/FAIL/SKIP: Self Descriptive.",
    "author": "Bharat Mukheja",
    "creationDate": "3/6/2018",
    "data": {
        "xAlign": true,
        "xColumn": "date_histo",
        "xLabel": "Time",
        "yColumn": "regression_name",
        "yTickGrid": false,
        "brush":8,
        "brushArea":3,
        "yLabel": "Regression Name",
        "legendColumn": "result",
        "colorColumn": "result",
        "heatmapColor": {
            "SKIP": "gray",
            "FAIL": "red",
            "PASS": "green",
            "": "white"
        },
        "legend": {
            "show": true,
            "orientation": "horizontal",
            "circleSize": 4,
            "labelOffset": 2
        },
        "tooltip": [
            { "column": "date_histo", "label": "Timestamp", "timeFormat": "%b %d"},
            { "column": "regression_name", "label": "Regression Name" },
            { "column": "build_number", "label": "Build Number" },
            { "column": "result", "label": "Result"},
            { "column": "link", "label": "Link" },
            { "column": "testbed", "label": "Testbed"}
        ],
        "tabifyOptions":{
            "suiteAreasFile": "../files/testsuite_areas.json",
            "aql_area":"{{testsuiteArea}}"
        }
   },
   "listeners": [
        {
            "params": {
                "testsuiteTime": "date_histo",
                "regression_name":"regression_name",
                "testSuite":"testsuite",
                "aqlRegressionRegressionStarted":"time_regression_started"
            }
        }
    ],
    "query": "inf-regress-area-testsuite-heatmap"
}
