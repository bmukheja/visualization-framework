{
    "id": "aar-nsg-app-traffic-detail",
    "graph": "AreaGraph",
    "title": "NSG Application traffic detail on {{snsg}}",
    "description": "Application traffic history on NSG",
    "author": "Bharat Mukheja",
    "creationDate": "10/20/2017",
    "data": {
        "dateHistogram": true,
        "xColumn": "ts",
        "xLabel": "Time",
        "yColumn": "SumofBytes",
        "yTickFormat": ",.2s",
        "yLabel": "Traffic",
        "stroke": {
            "width": "2px"
        },
        "yTicks": 5,
        "linesColumn": "L7Classification",
        "legend": {
            "orientation": "vertical",
            "show": true,
            "circleSize": 5,
            "labelOffset": 5
        },
        "tooltip": [
            { "column": "L7Classification", "label": "Application"},
            { "column": "SumofBytes", "label": "Data Usage", "format": ",.2s"},
            { "column": "ts", "label": "Timestamp", "timeFormat": "%b %d, %y %X"}
        ]
    },
    "listeners": [
    {
        "params": {
            "app": "L7Classification"
        }
    }],
    "filterOptions": {
        "Traffic Type": {
            "parameter": "app_trafficType",
            "default": "EgressBytes",
            "options": [
                {
                    "label": "Download",
                    "value": "EgressBytes",
                    "default": true
                },
                {
                    "label": "Upload",
                    "value": "IngressBytes"
                }
            ]
        }
    },
    "query": "aar-nsg-app-traffic-detail"
}
