{
    "id": "aar-nsg-ike-tunnel-packet-traffic-linechart",
    "graph": "MultiLineGraph",
    "title": "Packet Count",
    "description": "Line graph displays the throughput of the tunnel over the selected path",
    "author": "Bharat Mukheja",
    "creationDate": "3/6/2018",
    "data": {
        "dateHistogram": true,
        "xColumn": "date_histo",
        "yColumn": "{{ikePacketTypeColumn}}",
        "yTickFormat": ".2s",
        "xLabel": "Time",
        "yLabel": "{{ikePacketType}} Packets",
        "linesColumn": "tunnel",
        "showNull": false,
        "stroke": {
          "color": "#f76159",
          "width": "2px"
        },
        "legend": {
            "orientation": "horizontal",
            "show": true,
            "circleSize": 5,
            "labelOffset": 5
        },
        "tooltip": [
            { "column": "tunnel","label":"Tunnel"},
            { "column": "yColumn","label":"Packet Count","format": ",.2s" },
            { "column": "throughput","label":"Throughput(B/s)","format": ",.4n"  }

        ]
    },
    "query": "aar-nsg-ike-tunnel-packet-traffic-linechart",
    "filterOptions": {
        "Packet Type": {
            "parameter": "ikePacketType",
            "default": "Tx",
            "options": [
                {
                    "label": "Tx",
                    "value": "Tx",
                    "default": true,
                    "forceOptions": {
                        "ikePacketTypeColumn": "TxPacketsCount"
                    }
                },
                {
                    "label": "Rx",
                    "value": "Rx",
                    "forceOptions": {
                        "ikePacketTypeColumn": "RxPacketsCount"
                    }
                }
            ]
        }
    }
}
