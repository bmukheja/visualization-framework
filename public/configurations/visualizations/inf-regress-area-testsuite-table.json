{
  "id": "inf-regress-area-testsuite-table",
  "graph": "Table",
  "title": "Full details of the selected '{{regression_name}}' regression started on: {{aqlRegressionRegressionStarted:call('epoch_to_datetime')}}",
  "description": "Colors of the result depict these outcomes.\n\nGray -> Skip\nRed -> Fail\nGreen -> Passed.",
  "author": "Ronak Shah",
  "data": {
    "columns": [
      {
        "column": "result",
        "label": "Result",
        "selection":true,
        "colors": {
          "0": "gray",
          "1": "red",
          "2": "green"
        }
      },
      {
        "column": "testcase",
        "label": "Test Case",
        "tooltip":{"column":"regression_link"}
      },
      {
        "column": "err_msg",
        "label": "Error Message",
        "tooltip":{"column":"regression_link"}
      }
    ]
  },
  "listeners": [
    {
      "params":{
        "testCase":"testcase"
      }
    }
  ],
  "query": "inf-regress-area-testsuite-table"
}