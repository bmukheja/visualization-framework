{
    "id":"aar-nsg-ike-tunnel-packet-traffic-linechart",
    "title":"TBD",
    "service":"elasticsearch",
    "query":{
        "index":"{{index:nuage_ike_stats}}",
        "type":"{{type:nuage_doc_type}}",
        "body":{
            "sort":{
              "timestamp":"desc"
            },
            "query":{
                  "bool":{
                    "must":[
                        {
                            "range":{
                                "timestamp":{
                                    "gte":"{{startTime:now-15m}}",
                                    "lte":"{{endTime:now}}",
                                    "format":"epoch_millis"
                                }
                            }
                        },
                        {
                            "term": {
                                 "EnterpriseName": "{{enterpriseName:test_org}}"
                            }
                        },
                        {
                            "term": {
                                "SourceNSG": "{{snsg}}"
                            }
                        },
                        {
                            "term": {
                                "TunnelName": "{{tunnel}}"
                            }
                        }
                    ]
                }
            }
        }
    }
}