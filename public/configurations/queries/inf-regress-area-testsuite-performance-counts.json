{
  "id": "inf-regress-area-testsuite-performance-counts",
  "author": "Bharat Mukheja",
  "service": "elasticsearch",
  "tabify": "testSuitePerformanceCounts",
  "query": {
    "index": "nuage_regress",
    "type": "nuage_doc_type",
    "body": {
      "sort": [
        {
          "time_regression_started": {
            "order": "desc"
          }
        }
      ],
      "size": 0,
      "query": {
        "bool": {
          "must": [
            {
              "range": {
                "time_regression_submitted": {
                  "gte": "{{testsuiteTime}}||-5m",
                  "lte": "{{testsuiteTime}}||+1435m",
                  "format": "epoch_millis"
                }
              }
            },
            {
              "regexp":{
                "version.vrs":"[0-9].[0-9]/[0-9.]+"
              }
            },
            {
              "regexp":{
                "version.vsd":"[0-9].[0-9]/[0-9.]+"
              }
            },
            {
              "regexp":{
                "version.vsc":"[0-9].[0-9]/[0-9ISBN.]+"
              }
            }
          ],
          "filter": [
            {
              "term": {
                "testsuite": "{{testSuite}}"
              }
            },
            {
              "term": {"project": "{{branch}}"}
            }
          ]
        }
      },
      "_source": {
        "excludes": []
      },
      "aggs": {
        "result": {
          "terms": {
            "field": "result"
          }
        }
      }
    }
  }
}