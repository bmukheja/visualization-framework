{
    "id": "inf-regress-area-testcase-table",
    "author": "Bharat Mukheja",
    "service": "elasticsearch",
    "tabify": "regressExtractRunLevel",
    "query": {
      "index": "nuage_regress",
      "type": "nuage_doc_type",
      "body": {
        "sort": [
          { "time_regression_started" : {"order" : "desc"}}
        ],
        "size": 5,
        "query": {
          "bool": {
            "must": [
              {
                "range":{
                  "time_regression_submitted":{
                    "lte":"{{testsuiteTime}}||+1435m",
                    "format":"epoch_millis"
                  }
                }
              },
              {
                "regexp":{
                  "version.vrs":"[0-9].[0-9]/[0-9.]+"
                }
              },
              {
                "regexp":{
                  "version.vsd":"[0-9].[0-9]/[0-9.]+"
                }
              },
              {
                "regexp":{
                  "version.vsc":"[0-9].[0-9]/[0-9ISBN.]+"
                }
              }
            ],
            "filter": [
              {"term": { "testcase": "{{testCase}}"}},
              {"term": {"project": "{{branch}}"}}
            ]
          }
        }
      }
    }
  }