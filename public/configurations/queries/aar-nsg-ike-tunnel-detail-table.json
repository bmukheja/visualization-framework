{
    "id":"aar-nsg-ike-tunnel-detail-table",
    "title":"NAT Probe Results at time",
    "service":"elasticsearch",
    "query":{
        "index":"{{index:nuage_natt}}",
        "type":"{{type:nuage_doc_type}}",
        "body":{
            "size": "{{ts_size:4}}",
            "sort": [
                {
                    "timestamp": {
                        "order": "desc"
                    }
                }
            ],
            "query": {
                "bool": {

                    "filter": {
                        "bool":{
                            "must":[
                                {
                            "range": {
                                "timestamp": {
                                    "lte": "{{event_timestamp}}",
                                    "format": "epoch_millis"
                                }
                            }
                        },
                        {
                            "term":{
                                "SourceNSG":"{{snsg}}"
                            }
                        },
                        {
                            "term":{
                                "DestinationNSG":"{{dnsg}}"
                            }
                        },
                                {
                                    "exists":{
                                        "field":"Reason"
                                    }
                                }
                            ]
                        }
                    }
                }
            }
        }
    }
}
