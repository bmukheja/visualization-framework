{
    "id": "aar-nsg-natt-heatmap",
    "title": "NAT-T Reachability HeatMap",
    "service": "elasticsearch",
    "query": {
        "index": "{{index:nuage_ike_probe_status}}",
        "type": "{{type:nuage_doc_type}}",
        "body": {
            "sort": [
                {
                    "timestamp": {
                        "order": "desc"
                    }
                }
            ],
            "query": {
                "bool": {
                    "must": [
                        {
                            "range": {
                                "timestamp": {
                                    "gte":"{{startTime:now-15m}}",
                                    "lte":"{{endTime:now}}",
                                    "format": "epoch_millis"
                                }
                            }
                        }
                    ],
                    "filter": {
                        "bool":{
                            "must":[
                                {
                                    "term": {
                                        "SourceNSG":"{{snsg}}"
                                     }
                                }
                            ]
                        }
                    }
                }
            }
        },
        "aggs":{
        "date_histo":{
            "date_histogram": {
                        "field": "timestamp",
                        "interval": "2m"
                }
            }
        }
    }
}
