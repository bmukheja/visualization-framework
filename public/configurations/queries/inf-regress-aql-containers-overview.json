{
    "id":"inf-regress-aql-containers-overview",
    "title":"",
    "service":"elasticsearch",
    "tabify": "aqlSimpleText",
    "query":{
        "index":"nuage_aql",
        "type":"{{type:nuage_doc_type}}",
        "body":{    
            "size": 0,
            "query":{
                "bool":{
                    "must":[
                        {
                            "term": {
                                "Area":"containers"
                            }
                        },
                        {
                            "term": {
                                "branch": "{{branch:0.0}}"
                            }
                        },
                        {
                            "range":{
                                "timestamp":{
                                    "gte":"{{startTime:now-1d}}",
                                    "lte":"{{endTime:now}}",
                                    "format":"epoch_millis"
                                }
                            }
                        }
                    ]
                }
            },
            "aggs": {
                "area": {
                    "terms": {
                        "field": "Area",
                        "size":2000
                    },
                    "aggs": {
                        "suites": {
                            "terms": {
                                "field": "RegressionName",
                                "size": 100
                            },
                            "aggs": {
                                "run_count": {
                                    "filters": {
                                        "filters": {
                                            "NotRun": {
                                                "term": {
                                                    "TimesRun": 0
                                                }
                                            },
                                            "Total": {
                                                "exists": {
                                                    "field": "TimesRun"
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}