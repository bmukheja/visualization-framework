{
    "id":"aar-nsg-ike-probe-detail-table",
    "title":"NAT Probe Results at time",
    "service":"elasticsearch",
    "query":{
        "index":"{{index:nuage_ike_probe_status}}",
        "type":"{{type:nuage_doc_type}}",
        "body":{
            "sort": [
                {
                    "timestamp": {
                        "order": "desc"
                    }
                }
            ],
            "query": {
                "bool": {
                    "filter": {
                        "bool":{
                            "must":[
                                {
                            "range": {
                                "timestamp": {
                                    "lte": "{{ike_probe_timestamp}}",
                                    "gte": "{{ike_probe_timestamp}}||-2m",
                                    "format": "epoch_millis"
                                }
                            }
                        },
                        {
                            "term":{
                                "SourceNSG":"{{snsg}}"
                            }
                        },
                        {
                            "term": {
                                "TunnelName": "{{tunnel}}"
                            }
                        }
                            ]
                        }
                    }
                }
            }
        }
    }
}
