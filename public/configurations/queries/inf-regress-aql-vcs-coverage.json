{
  "id": "inf-regress-aql-vcs-coverage",
  "title": "",
  "service": "elasticsearch",
  "tabify": "aqlCoverage",
  "query": {
    "index": "nuage_regress",
    "type": "nuage_doc_type",
    "body": {
      "size": 0,
      "query": {
        "bool": {
          "must": [
            {
              "regexp": {
                "aql_area": "(dc_|wbx|vmware|contain).*"
              }
            },
            {
              "wildcard": {
                  "_id": "*@*"
              }
          },
          {
              "regexp": {
                  "version.vrs": "[0-9].[0-9]/[0-9.]+"
              }
          },
          {
              "regexp": {
                  "version.vsd": "[0-9].[0-9]/[0-9.]+"
              }
          },
          {
              "regexp": {
                  "version.vsc": "[0-9].[0-9]/[0-9ISBN.]+"
              }
          },
          {
              "term": {
                  "project": "{{branch}}"
              }
          },
          {
              "range": {
                  "time_regression_started": {
                      "gte": "{{startTime:now-1d}}",
                      "lte": "{{endTime:now}}",
                      "format": "epoch_millis",
                      "time_zone":"America/Los_Angeles"
                  }
              }
          }
          ]
        }
      },
      "aggs": {
        "suites": {
          "cardinality": {
            "field": "RegressionName"
          }
        }
      }
    }
  }
}